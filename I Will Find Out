Ifoundout={"n":0,"bd":0,"pw":0 }
youwilltellme=[]
Ineedtoask=["What is your name fine human?","Thanks for that! I will need your Birthday next!","Awesome! Please create a password to finish this off!"]
for x in(Ineedtoask):
    y=input(x)
    youwilltellme.append(y)
Ifoundout["n"]=youwilltellme[0]
Ifoundout["bd"]=youwilltellme[1]
Ifoundout["pw"]=youwilltellme[2]
print(youwilltellme)
print("nice, nice baby")
print(youwilltellme[2])
print(Ifoundout)
